#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#include "headers/tuple.h"

void addNumber(struct TUPLE *tuple, char num)
{
	tuple->numbers[strlen(tuple->numbers)] = num;
}
void addCharacter(struct TUPLE *tuple, char c)
{
	tuple->characters[strlen(tuple->characters)] = c;
}
